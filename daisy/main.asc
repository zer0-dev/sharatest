function objInvToStr(Inventory) {
    var str = "";

    for (var tab in Inventory) {
        for (var i = 0; i < Inventory[tab].length; i++) {
            var ms = "";
            for (var key in Inventory[tab][i]) {
                if (key != "tab") {
                    ms = ms + key + ">" + Inventory[tab][i][key] + "|";
                }
            }
            str += ms + "<";
        }
        str = str.slice(0, -1) + ";";
    }

    return str.slice(0, -1);
}

